                    </div>
                    <div class="transfers">
                        <div class="transfer" style="margin-bottom: 0px; margin-top: 5px">
                            <div class="transfer-logo">
                                
                            </div>
                            <dl class="transfer-details">
                                <div>
                                   Наименование
                                </div>
                                <div>
                                  Откуда
                                </div>
                                <div>
                                    Куда
                                </div>
                                <div>
                                   Пользователь
                                </div>
                                <div>
                                    Дата
                                </div>
                            </dl>
                            <div class="transfer-number">
                            </div>
                        </div>
                        @foreach(var history in Model)
                        {
                            <div class="transfer" style="margin-bottom: 0px; margin-top: 5px">
                                <div class="transfer-logo">
                                    <img src= "~/imgs/9.png"/>
                                </div>
                                <dl class="transfer-details">
                                    <div>
                                        @history.Device.Name
                                    </div>
                                    <div>
                                       @history.FromPlace
                                    </div>
                                    <div>
                                        @history.ToPlace
                                    </div>
                                    <div>
                                       @history.UserId
                                    </div>
                                    <div>
                                        @history.DateTime
                                    </div>
                                </dl>
                                <div class="transfer-number">
                                </div>
                            </div>
                        }

                    </div>
                </section>
            </div>
            <div class="app-body-sidebar" style="color:black">
                <section class="payment-section" style="color:black">
                    <h2 style="margin-bottom:0px">Предстоящие задачи</h2>
                    
